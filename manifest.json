{
	"manifest_version": 2,

	"icons": {
		"24": "assets/24.png",
		"128": "assets/128.png"
	},

	"name": "Asana for scrum",
	"description": "This extension calculates total hours of sprint",
	"version": "1.0",

	"browser_action": {
		"default_icon": "assets/icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
		"http://*/",
		"https://*/"
	],

	"content_scripts": [
		{
			"matches": ["https://*/*"],
			"js": [
				"js/vendor/jquery-1.11.2.min.js",
				"js/main.js"
			]
		}
	]
}
