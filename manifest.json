{
	"manifest_version": 2,
	"name": "Asana for Scrum",
	"version": "1.3.9",


	"description": "This extension calculates total hours of sprint",
	"icons": {
		"24": "assets/24.png",
		"128": "assets/128.png"
	},

	"author": "Aram Baghdasaryan",

	"browser_action": {
		"default_icon": "assets/icon.png",
		"default_popup": "popup.html"
	},

	"permissions": [
		"tabs",
		"storage",
		"*://*/"
	]
}
